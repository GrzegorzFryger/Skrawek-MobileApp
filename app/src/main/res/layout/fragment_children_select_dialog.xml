<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="8dp"
    android:background="@drawable/routed_table"
    tools:context=".ui.children.ChildrenSelectDialogFragment">


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/list"
        android:name="pl.edu.pjatk.pamo.skrawek.ui.children.ChildrenSelectDialog"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layoutManager="LinearLayoutManager"
        tools:context=".ui.children.ChildrenSelectDialogFragment"
        tools:listitem="@layout/item_list_child_select" />


</FrameLayout>